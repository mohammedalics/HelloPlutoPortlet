<?xml version="1.0" encoding="UTF-8"?>
<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" 
version="2.0" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" 
id="poc.portlet.helloportlet.HelloPortlet.7aed100ae3">

	<portlet>
		<portlet-name>HelloWorldPortlet</portlet-name>
		<display-name xml:lang="en">
     HelloPortlet
		</display-name>
		<portlet-class>
     poc.portlet.helloportlet.HelloPortlet
		</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<portlet-info>
			<title>Hello Portlet</title>
		</portlet-info>
	</portlet>

	<portlet>
		<portlet-name>TitleChangedPortlet</portlet-name>
		<display-name xml:lang="en">
     TitleChangedPortlet
		</display-name>
		<portlet-class>
     poc.portlet.titlechangedportlet.TitleChangedPortlet
		</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<portlet-info>
			<title>Title Changed Portlet</title>
		</portlet-info>
		<resource-bundle>message</resource-bundle>
	</portlet>


	<portlet>
		<portlet-name>ProgrammaticTitleChangePortlet</portlet-name>
		<display-name xml:lang="en">
     TitleChangedPortlet
		</display-name>
		<portlet-class>
     poc.portlet.titlechangedportlet.TitleChangedPortlet
		</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<portlet-info>
			<title>Title Changed Portlet</title>
		</portlet-info>
		<resource-bundle>message</resource-bundle>
	</portlet>

    <portlet>
        <display-name>Request Attributes</display-name>
        <portlet-name>RequestParameterPortlet</portlet-name>
        <portlet-class>poc.portlet.requestparameter.RequestParameterPortlet</portlet-class>
        <description>Print out all attributes located at the PortletRequest</description>
        <portlet-info>
            <title>Request Attributes</title>
            <keywords>request, attributes</keywords>
            <short-title>Request Attributes</short-title>
        </portlet-info>
    </portlet>
    
    <portlet>
        <display-name>Action Request Reader</display-name>
        <portlet-name>ActionRequestReaderPortlet</portlet-name>
        <portlet-class>poc.portlet.actionrequestreader.ActionRequestReaderPortlet</portlet-class>
        <description>test action request and file upload</description>
        <portlet-info>
            <title>Action Request file upload</title>
  
        </portlet-info>
    </portlet>
    
    
       <portlet>
        <display-name>Render Response Namespace One</display-name>
        <portlet-name>RenderResponseNamespaceOne</portlet-name>
        <portlet-class>poc.portlet.renderresponse.RenderResponseNamespaceOnePortlet</portlet-class>
        <description>Render Response Namespace One</description>
        <portlet-info>
            <title>Render Response Namespace One</title>
  
        </portlet-info>
    </portlet>
    
        <portlet>
        <display-name>Render Response Namespace Two</display-name>
        <portlet-name>RenderResponseNamespaceTwo</portlet-name>
        <portlet-class>poc.portlet.renderresponse.RenderResponseNamespaceTwoPortlet</portlet-class>
        <description>Render Response Namespace Two</description>
        <portlet-info>
            <title>Render Response Namespace Two</title>
  
        </portlet-info>
    </portlet>
    
    <portlet>
        <display-name>Hello Context Portlet</display-name>
        <portlet-name>HelloContextPortlet</portlet-name>
        <portlet-class>poc.portlet.portletcontext.HelloContextPortlet</portlet-class>
        <description>Hello Context Portlet</description>
        <portlet-info>
            <title>Hello Context Portlet</title>
  
        </portlet-info>
        <init-param>
            <name>portletContextParam</name>
            <value>PortletContextParamValue</value>
        </init-param>
    </portlet>
     <portlet>
        <display-name>Access Resources Portlet</display-name>
        <portlet-name>AccessResourcesPortlet</portlet-name>
        <portlet-class>poc.portlet.accessresources.AccessResourcesPortlet</portlet-class>
        <description>Access Resources Portlet</description>
        <portlet-info>
            <title>Access Resources Portlet</title>
  
        </portlet-info>
    </portlet>
    
 	<portlet>
        <display-name>Session Portlet</display-name>
        <portlet-name>SessionPortlet</portlet-name>
        <portlet-class>poc.portlet.sessionportlet.SessionPortlet</portlet-class>
        <description>Session Portlet</description>
        <portlet-info>
            <title>Session Portlet</title>
  
        </portlet-info>
    </portlet>

	<default-namespace>
   http://localhost/
	</default-namespace>

</portlet-app>
